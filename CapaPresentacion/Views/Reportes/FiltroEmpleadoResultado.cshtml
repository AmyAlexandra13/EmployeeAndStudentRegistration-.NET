@model IEnumerable<CapaEntidad.FiltroIDEmpleado_Result>

@{
    ViewBag.Title = "FiltroEmpleadoResultado";
    Layout = "~/Views/Shared/_MenuLayout.cshtml";
}

<h2 class="text-center btn-warning">Filtrando por ID de empleado</h2>


    <table class="table table-striped table-hover">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdDocumento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoDocumento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DptoOrigen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DptoDestino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Secuencia)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdDocumento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IdUser)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TipoDocumento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DptoOrigen)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DptoDestino)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Fecha)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Secuencia)
                </td>

            </tr>
        }

    </table>

    <p>
        @Html.ActionLink("Volver atras", "ReporteDocumento", new { area = "" }, new { @class = "btn btn-primary" })
    </p>